{
  "name": "@careersignal/agents",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./profile/*": {
      "types": "./dist/profile/*.d.ts",
      "default": "./dist/profile/*.js"

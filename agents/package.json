    "clean": "rimraf dist",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@careersignal/llm": "*",
    "@careersignal/schemas": "*",
    "pdf-parse": "^1.1.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/pdf-parse": "^1.1.4",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0"
  }
}
